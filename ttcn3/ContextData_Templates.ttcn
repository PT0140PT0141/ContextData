/**
 * @author  PT28/PT0141 
 * @desc    This module provides templates for Charging
 * @see     Electronic fee collection (EFC) — Conformity evaluation of Front End and Back End equipment to ISO 17575-3 — Part 1: Test suite structure and test purpose
 * @see     Electronic fee collection (EFC) — Evaluation of equipment for conformity to CEN ISO/TS 17575-3 — Part 1: Test suite structure and test purpose
 * @remark  See Annex A - EFC data type specifications
 * @version $id$
 */
module ContextData_Templates language "TTCN-3:2010 Advanced Parameterization" {
    // EFC
    /**
     * @desc    [17575-1:2016], Charging
     * @see     Annex A (normative) EFC data type specifications
     */
    import from EfcAutonomousCharging language "ASN.1:1997" all;
    /**
     * @desc    [17575-3:2016], Context data
     * @see     A.2 Data specifications
     */
    import from EfcAutonomousContextData language "ASN.1:1997" all;
    import from EfcDsrcApplication language "ASN.1:1997" all;
    import from EfcLac language "ASN.1:1997" all;
     
     // LibEfc_Common
    /**
     * @desc    TTCN-3 does not support ASN.1 parameterization (ETSI ES 201 873-7 V4.5.1 Clause 10)
     *          Common_Parameterised_Asn1_Data TTCN-3 module is a workaround
     */
    import from Common_Parameterised_Asn1_Data all;
    import from Common_Templates all;
    import from Common_TypesAndValues all;
    import from Common_Pixits all;
    
    // ContextData
    import from ContextData_TypesAndValues all;
    import from ContextData_Pixits all;

    /**
     * @desc    Template for Context Data ADU header #3..10
     * @see     [16410-1:2016], Annex C.1.1: General structure of Context Data - Table C.1
     * @see     [16410-1:2016], Annex C.2.1: Context Data - Table C.4: ContextData1 to Table C.11: ContextData25
     */
    template (value) Iso17575_3AduHeader m_aduHeader
    modifies m_aduHeader_default := {
        aduSequenceNumber := (valueof(m_aduHeader_default.aduSequenceNumber) + 1) mod 4294967296
    } // End of template m_aduHeader

    group Charge_Report {
        
        /**
         * @desc    Group of Context Date describes in [16410-1:2016], Annex C.2: Data structure - Table C.4 —  ContextData1
         * @see     [17575-1:2016], Charging for data element descriptions
         * @see     [16410-1:2016], Table C.4 —  ContextData1
         */
        group ContextData1 {
            /**
             * @desc    Main template for ContextData1
             * @param   p_tollContext ADU header toll context
             * @see     [16410-1:2016], Table C.4 —  ContextData1
             */
            template (value) Iso17575_3AduBody m_contextData1(
                                                              in Provider p_tollContext := PX_TOLL_CONTEXT_ID_1
            ) modifies m_contextData_default := {
                tollContextOverview             := m_optionally_Signed_Unsecured<TollContextOverview>(m_tollContextOverview_default(p_tollContext))
            }
            
        } // End of group ContextData1
        
        /**
         * @desc    Group of Context Date describes in [16410-1:2016], Annex C.2: Data structure - Table C.5 —  ContextData1
         * @see     [17575-1:2016], Charging for data element descriptions
         * @see     [16410-1:2016], Table C.5 —  ContextData13
         */
        group ContextData13 {
            /**
             * @desc    Main template for ContextData13
             * @see     [16410-1:2016], Table C.5 —  ContextData13
             */
            template (value) Iso17575_3AduHeader m_aduHeader_contextData13
            modifies m_aduHeader_default := {
                tollContext := PX_TOLL_CONTEXT_ID_13
            } // End of template m_aduHeader_contextData13
    
            /**
             * @desc    Main template for ContextData13
             * @see     [16410-1:2016], Table C.5 —  ContextData13
             */
            template (value) Iso17575_3AduBody m_contextData13(
                                                               in Provider p_tollContext := PX_TOLL_CONTEXT_ID_13
            ) modifies m_contextData1 := { 
                tollContextOverview       := m_optionally_Signed_Unsecured<TollContextOverview>(m_tollContextOverview_default(p_tollContext)),
                chargeReportConfiguration := m_optionally_Signed_Unsecured<ChargeReportConfigurations>({ m_chargeReportConfiguration_contextData13 })
            } // End of template m_contextData13
    
            /**
             * @desc    ChargeReportConfiguration for Context Data #13
             * @see     [17575-3:2016], Context data - Clause 8.3.5.2 EFC attribute ChargeReportConfiguration
             * @see     [16410-1:2016], Table C.5 —  ContextData13
             */
            template (value) ChargeReportConfiguration m_chargeReportConfiguration_contextData13
            modifies m_chargeReportConfiguration_default := {
                chargeReportContent := m_chargeReportContent_contextData13,
                chargeReportConfigurationVersion := m_chargeReportConfigurationVersion_contextData13
            } // End of template m_chargeReportConfiguration_contextData13
    
            /**
             * @desc    chargeReportConfigurationVersion default template
             * @see     [17575-3:2016], Context data - Clause 8.3.5.2   EFC attribute ChargeReportConfiguration
             * @see     [16410-1:2016], Table C.5 —  ContextData13
             */
            template (value) VersionAndValidity m_chargeReportConfigurationVersion_contextData13
            modifies m_chargeReportConfigurationVersion_default := {
                validFrom := PX_VALID_FROM_13
            } // End of template m_chargeReportConfigurationVersion_contextData13
    
            /**
             * @desc    ChargeReport template for Context Data #13
             * @see     [17575-3:2016], Context data - Clause 8.3.5.2.1 Data type ChargeReport
             * @see     [16410-1:2016], Table C.5 —  ContextData13
             */
            template (value) ChargeReportContent m_chargeReportContent_contextData13
            modifies m_chargeReportContent_default := {
                useOfCrAuthentication       := false,
                vehicleLPNr                 := true,
                paymentMeans                := true,
                chargeReportFinalRecipient  := true,
                versionInfo                 := true,
                sumVatForThisSession        := true,
                accountStatus               := true,
                chargeReportCounter         := true,
                mileage                     := true,
                listOfCccAttributes         := true
            } // End of template m_chargeReportContent_contextData13
            
            /**
             * @desc ChargeReport template compliant with Context Data #13
             */
            template ChargeReport mw_chargeReport_contextData13
            modifies mw_chargeReport_default := {
                vehicleLPNr             := complement(?),
                paymentMeans            := complement(?),
                tollContext             := complement(?),
                reportPeriod            := ?,
                versionInfo             := complement(?),
                usageStatementList      := { },
                sumVatForThisSession    := complement(?),
                accountStatus           := complement(?),
                chargeReportCounter     := complement(?),
                mileage                 := complement(?),
                listOfCccAttributes     := *
            } // End of template mw_chargeReport_contextData13
            
        } // End of group ContextData13
        
        /**
         * @desc    Group of Context Date describes in [16410-1:2016], Annex C.2: Data structure - Table C.6
         * @see     [17575-1:2016], Charging for data element descriptions
         * @see     [16410-1:2016], Table C.6 —  ContextData20
         */
        group ContextData20 {
            /**
             * @desc    Main template for ContextData20
             * @param   p_tollContextPartitionId The identifier of the respective toll context partition for which the overview information is provided
             * @see     [16410-1:2016], Table C.6 —  ContextData20
             */
            template (value) Iso17575_3AduBody m_contextData20(
                                                               in Provider p_tollContext := PX_TOLL_CONTEXT_ID_20,
                                                               in TollContextPartitionId p_tollContextPartitionId := PX_TOLLCONTEXTPARTITIONID_1
            ) modifies m_contextData1 := {
                tollContextOverview       := m_optionally_Signed_Unsecured<TollContextOverview>(
                    m_tollContextOverview_default(
                        p_tollContext,
                        {
                            p_tollContextPartitionId,       // Partition #1
                            p_tollContextPartitionId + 1    // Partition #2
                        }
                    )
                ),
                tollContextPartitionOverview := m_optionally_Signed_Unsecured<TollContextPartitionOverviews>(
                    {
                        m_tollContextPartitionOverview_precedenceLevel(p_tollContextPartitionId),      // Partition #1
                        m_tollContextPartitionOverview_precedenceLevel(p_tollContextPartitionId + 1)   // Partition #2
                    }
                ),
                chargeReportConfiguration := m_optionally_Signed_Unsecured<ChargeReportConfigurations>(
                    { 
                        m_chargeReportConfiguration_contextData20(        // Partition #1
                            p_tollContextPartitionId,
                            m_chargeReportContent_contextData20_1,
                            m_usageStatementContent_contextData20_1
                        ),
                        m_chargeReportConfiguration_contextData20(        // Partition #2
                            p_tollContextPartitionId,
                            m_chargeReportContent_contextData20_2,
                            m_usageStatementContent_contextData20_1
                        )
                    }
                )
            } // End of template m_contextData20
            
            /**
             * @desc    ChargeReportConfiguration for Context Data #20 
             * @see     [17575-3:2016], Context data - Clause 8.3.5.2 EFC attribute ChargeReportConfiguration
             * @see     [16410-1:2016], Table C.6 —  ContextData20
             */
            template (value) ChargeReportConfiguration m_chargeReportConfiguration_contextData20(
                                                                                                 in TollContextPartitionId p_tollContextPartitionId,
                                                                                                 in template (value) ChargeReportContent p_chargeReportContent_contextData,
                                                                                                 in template (value) UsageStatementContent p_usageStatementContent
            ) modifies m_chargeReportConfiguration_default := {
                applicablePartitions := 
                    {
                        p_tollContextPartitionId,       // Partition #1
                        p_tollContextPartitionId + 1    // Partition #2
                    },
                chargeReportContent := p_chargeReportContent_contextData,
                usageStatementContent := p_usageStatementContent 
            } // End of template m_chargeReportConfiguration_contextData20
    
            /**
             * @desc    ChargeReport template for Context Data #20 - Partition #1
             * @see     [17575-3:2016], Context data - Clause 8.3.5.2.1 Data type ChargeReport
             * @see     [16410-1:2016], Table C.6 —  ContextData20
             */
            template (value) ChargeReportContent m_chargeReportContent_contextData20_1
            modifies m_chargeReportContent_default := {
                chargeReportCounter         := true
            } // End of template m_chargeReportContent_contextData20_1
            
            /**
             * @desc    UsageStatementContent template for Context Data #20 - Partition #1
             * @see     [17575-3:2016], Clause 8.5.5.3.3 Data type UsageStatementContent
             * @see     [16410-1:2016], Table C.6 —  ContextData20
             */
            template (omit) UsageStatementContent m_usageStatementContent_contextData20_1 
            modifies  m_usageStatementContent_default := {
                useOfUsageStatementAuthentication   := true,
                usageStatementId                    := true,
                tollContext                         := true,
                chargeReportFinalRecipient          := true
            } // End of template m_usageStatementContent_contextData20_1
            
            /**
             * @desc    ChargeReport template for Context Data #20 - Partition #2
             * @see     [17575-3:2016], Context data - Clause 8.3.5.2.1 Data type ChargeReport
             * @see     [16410-1:2016], Table C.6 —  ContextData20
             */
            template (value) ChargeReportContent m_chargeReportContent_contextData20_2
            modifies m_chargeReportContent_default := {
                obeId                       := false,
                vehicleLPNr                 := true,
                versionInfo                 := true,
                chargeReportCounter         := true
            } // End of template m_chargeReportContent_contextData20_2
            
            /**
             * @desc ChargeReport template compliant with Context Data #20
             * @see     [16410-1:2016], Table C.6 —  ContextData20
             */
            template ChargeReport mw_chargeReport_contextData20_1(
                                                                  in Provider p_tollContext := PX_TOLL_CONTEXT_ID_20
            ) modifies mw_chargeReport_default := {
                usageStatementList := superset(
                    mw_usageStatement_usageStatement<octetstring>(mw_usageStatement_tollContext<octetstring>(p_tollContext))
                )
            } // End of template mw_chargeReport_contextData20_1
            /**
             * @desc ChargeReport template compliant with Context Data #20
             * @see     [16410-1:2016], Table C.6 —  ContextData20
             */
            template ChargeReport mw_chargeReport_contextData20_2(
                                                                  in Provider p_tollContext := PX_TOLL_CONTEXT_ID_20
            ) modifies mw_chargeReport_contextData20_1 := {
                obeId := *,
                vehicleLPNr := ?
            } // End of template mw_chargeReport_contextData20_2
            
        } // End of group ContextData20
        
        /**
         * @desc    Group of Context Date describes in [16410-1:2016], Annex C.2: Data structure - Table C.7
         * @see     [17575-1:2016], Charging for data element descriptions
         * @see     [16410-1:2016], Table C.7 —  ContextData21
         */
        group ContextData21 {
            /**
             * @desc    Main template for ContextData21
             * @param   p_tollContextPartitionId The identifier of the respective toll context partition for which the overview information is provided
             * @see     [16410-1:2116], Table C.7 —  ContextData21
             */
            template (value) Iso17575_3AduBody m_contextData21(
                                                               in Provider p_tollContext := PX_TOLL_CONTEXT_ID_21,
                                                               in TollContextPartitionId p_tollContextPartitionId := PX_TOLLCONTEXTPARTITIONID_1
            ) modifies m_contextData20 := {
                tollContextPartitionOverview := m_optionally_Signed_Unsecured<TollContextPartitionOverviews>(
                    {
                        m_tollContextPartitionOverview_precedenceLevel(      // Partition #1
                            p_tollContextPartitionId,
                            -,
                            -,
                            -,
                            10
                        ),
                        m_tollContextPartitionOverview_precedenceLevel(      // Partition #2
                            p_tollContextPartitionId + 1,
                            -,
                            -,
                            -,
                            8                                                // P1 > P2
                        )
                     }
                ),
                chargeReportConfiguration := m_optionally_Signed_Unsecured<ChargeReportConfigurations>(
                    { 
                        m_chargeReportConfiguration_contextData21(        // Partition #1
                            p_tollContextPartitionId,
                            m_chargeReportContent_contextData21_1,
                            m_usageStatementContent_contextData21_1
                        ),
                        m_chargeReportConfiguration_contextData21(        // Partition #2
                            p_tollContextPartitionId,
                            m_chargeReportContent_contextData21_2,
                            m_usageStatementContent_contextData21_1
                        )
                    }
                )
            } // End of template m_contextData21
            
            /**
             * @desc    ChargeReportConfiguration for Context Data #21 
             * @see     [17575-3:2116], Context data - Clause 8.3.5.2 EFC attribute ChargeReportConfiguration
             * @see     [16410-1:2116], Table C.7 —  ContextData21
             */
            template (value) ChargeReportConfiguration m_chargeReportConfiguration_contextData21(
                                                                                                 in TollContextPartitionId p_tollContextPartitionId,
                                                                                                 in template (value) ChargeReportContent p_chargeReportContent_contextData,
                                                                                                 in template (value) UsageStatementContent p_usageStatementContent
            ) modifies m_chargeReportConfiguration_default := {
                applicablePartitions := 
                    {
                        p_tollContextPartitionId,       // Partition #1
                        p_tollContextPartitionId + 1    // Partition #2
                    },
                chargeReportContent := p_chargeReportContent_contextData,
                usageStatementContent := p_usageStatementContent 
            } // End of template m_chargeReportConfiguration_contextData21
    
            /**
             * @desc    ChargeReport template for Context Data #21 - Partition #1
             * @see     [17575-3:2116], Context data - Clause 8.3.5.2.1 Data type ChargeReport
             * @see     [16410-1:2116], Table C.7 —  ContextData21
             */
            template (value) ChargeReportContent m_chargeReportContent_contextData21_1
            modifies m_chargeReportContent_default := {
                chargeReportCounter         := true
            } // End of template m_chargeReportContent_contextData21_1
            
            /**
             * @desc    UsageStatementContent template for Context Data #21 - Partition #1
             * @see     [17575-3:2116], Clause 8.5.5.3.3 Data type UsageStatementContent
             * @see     [16410-1:2116], Table C.7 —  ContextData21
             */
            template (omit) UsageStatementContent m_usageStatementContent_contextData21_1 
            modifies  m_usageStatementContent_default := {
                useOfUsageStatementAuthentication   := true,
                usageStatementId                    := true,
                tollContext                         := true,
                chargeReportFinalRecipient          := true
            } // End of template m_usageStatementContent_contextData21_1
             
            /**
             * @desc    ChargeReport template for Context Data #21 - Partition #2
             * @see     [17575-3:2116], Context data - Clause 8.3.5.2.1 Data type ChargeReport
             * @see     [16410-1:2116], Table C.7 —  ContextData21
             */
            template (value) ChargeReportContent m_chargeReportContent_contextData21_2
            modifies m_chargeReportContent_default := {
                versionInfo                 := true,
                chargeReportCounter         := true
            } // End of template m_chargeReportContent_contextData21_2
             
            /**
             * @desc ChargeReport template compliant with Context Data #21
             * @see     [16410-1:2116], Table C.7 —  ContextData21
             */
            template ChargeReport mw_chargeReport_contextData21_1(
                                                                  in Provider p_tollContext := PX_TOLL_CONTEXT_ID_21
            ) modifies mw_chargeReport_default := {
                usageStatementList := superset(
                    mw_usageStatement_usageStatement<octetstring>(mw_usageStatement_tollContext<octetstring>(p_tollContext))
                )
            } // End of template mw_chargeReport_contextData21_1
            /**
             * @desc ChargeReport template compliant with Context Data #21
             * @see     [16410-1:2116], Table C.7 —  ContextData21
             */
            template ChargeReport mw_chargeReport_contextData21_2(
                                                                  in Provider p_tollContext := PX_TOLL_CONTEXT_ID_21
            ) modifies mw_chargeReport_contextData21_1 := {
                obeId := *,
                vehicleLPNr := ?
            } // End of template mw_chargeReport_contextData21_2
            
        } // End of group ContextData21
        
        /**
         * @desc    Group of Context Date describes in [16410-1:2016], Annex C.2: Data structure - Table C.8
         * @see     [17575-1:2016], Charging for data element descriptions
         * @see     [16410-1:2116], Table C.8 —  ContextData22
         */
        group ContextData22 {
            
            /**
             * @desc    Main template for ContextData22
             * @param   p_tollContextPartitionId The identifier of the respective toll context partition for which the overview information is provided
             * @see     [16410-1:2216], Table C.8 —  ContextData22
             */
            template (value) Iso17575_3AduBody m_contextData22(
                                                               in Provider p_tollContext := PX_TOLL_CONTEXT_ID_22,
                                                               in TollContextPartitionId p_tollContextPartitionId := PX_TOLLCONTEXTPARTITIONID_1
            ) modifies m_contextData20 := {
                tollContextPartitionOverview := m_optionally_Signed_Unsecured<TollContextPartitionOverviews>(
                    {
                        m_tollContextPartitionOverview_precedenceLevel(      // Partition #1
                            p_tollContextPartitionId,
                            -,
                            -,
                            -,
                            10
                        ),
                        m_tollContextPartitionOverview_precedenceLevel(      // Partition #2
                            p_tollContextPartitionId + 1,
                            -,
                            -,
                            -,
                            8                                                // P1 > P2
                        )
                     }
                ),
                chargeReportConfiguration := m_optionally_Signed_Unsecured<ChargeReportConfigurations>(
                    { 
                        m_chargeReportConfiguration_contextData22(        // Partition #1
                            p_tollContextPartitionId,
                            m_chargeReportContent_contextData22_1,
                            m_usageStatementContent_contextData22_1
                        ),
                        m_chargeReportConfiguration_contextData22(        // Partition #2
                            p_tollContextPartitionId,
                            m_chargeReportContent_contextData22_2,
                            m_usageStatementContent_contextData22_1
                        )
                    }
                )
            } // End of template m_contextData22
            
            /**
             * @desc    ChargeReportConfiguration for Context Data #22 
             * @see     [17575-3:2216], Context data - Clause 8.3.5.2 EFC attribute ChargeReportConfiguration
             * @see     [16410-1:2216], Table C.8 —  ContextData22
             */
            template (value) ChargeReportConfiguration m_chargeReportConfiguration_contextData22(
                                                                                                 in TollContextPartitionId p_tollContextPartitionId,
                                                                                                 in template (value) ChargeReportContent p_chargeReportContent_contextData,
                                                                                                 in template (value) UsageStatementContent p_usageStatementContent
            ) modifies m_chargeReportConfiguration_default := {
                applicablePartitions := 
                    {
                        p_tollContextPartitionId,       // Partition #1
                        p_tollContextPartitionId + 1    // Partition #2
                    },
                chargeReportContent := p_chargeReportContent_contextData,
                usageStatementContent := p_usageStatementContent 
            } // End of template m_chargeReportConfiguration_contextData22
    
            /**
             * @desc    ChargeReport template for Context Data #22 - Partition #1
             * @see     [17575-3:2216], Context data - Clause 8.3.5.2.1 Data type ChargeReport
             * @see     [16410-1:2216], Table C.8 —  ContextData22
             */
            template (value) ChargeReportContent m_chargeReportContent_contextData22_1
            modifies m_chargeReportContent_default := {
                chargeReportCounter         := true
            } // End of template m_chargeReportContent_contextData22_1
            
            /**
             * @desc    UsageStatementContent template for Context Data #22 - Partition #1
             * @see     [17575-3:2216], Clause 8.5.5.3.3 Data type UsageStatementContent
             * @see     [16410-1:2216], Table C.8 —  ContextData22
             */
            template (omit) UsageStatementContent m_usageStatementContent_contextData22_1 
            modifies  m_usageStatementContent_default := {
                useOfUsageStatementAuthentication   := true,
                usageStatementId                    := true,
                tollContext                         := true,
                chargeReportFinalRecipient          := true
            } // End of template m_usageStatementContent_contextData22_1
             
            /**
             * @desc    ChargeReport template for Context Data #22 - Partition #2
             * @see     [17575-3:2216], Context data - Clause 8.3.5.2.1 Data type ChargeReport
             * @see     [16410-1:2216], Table C.8 —  ContextData22
             */
            template (value) ChargeReportContent m_chargeReportContent_contextData22_2
            modifies m_chargeReportContent_default := {
                versionInfo                 := true,
                chargeReportCounter         := true
            } // End of template m_chargeReportContent_contextData22_2
             
            /**
             * @desc ChargeReport template compliant with Context Data #22
             * @see     [16410-1:2216], Table C.8 —  ContextData22
             */
            template ChargeReport mw_chargeReport_contextData22_1(
                                                                  in Provider p_tollContext := PX_TOLL_CONTEXT_ID_22
            ) modifies mw_chargeReport_default := {
                usageStatementList := superset(
                    mw_usageStatement_usageStatement<octetstring>(mw_usageStatement_tollContext<octetstring>(p_tollContext))
                )
            } // End of template mw_chargeReport_contextData22_1
            /**
             * @desc ChargeReport template compliant with Context Data #22
             * @see     [16410-1:2216], Table C.8 —  ContextData22
             */
            template ChargeReport mw_chargeReport_contextData22_2(
                                                                  in Provider p_tollContext := PX_TOLL_CONTEXT_ID_22
            ) modifies mw_chargeReport_contextData22_1 := {
                obeId := *,
                vehicleLPNr := ?
            } // End of template mw_chargeReport_contextData22_2
            
        } // End of group ContextData22
        
        /**
         * @desc    Group of Context Date describes in [16410-1:2016], Annex C.2: Data structure - Table C.9
         * @see     [17575-1:2016], Charging for data element descriptions
         * @see     [16410-1:2016], Table C.9 —  ContextData23
         */
        group ContextData23 {
            /**
             * @desc    Main template for ContextData23
             * @see     [16410-1:2016], Table C.9 —  ContextData23
             */
            template (value) Iso17575_3AduBody m_contextData23(
                                                               in Provider p_tollContext := PX_TOLL_CONTEXT_ID_23,
                                                               in TollContextPartitionId p_tollContextPartitionId := PX_TOLLCONTEXTPARTITIONID_1
            ) modifies m_contextData1 := { 
                tollContextOverview       := m_optionally_Signed_Unsecured<TollContextOverview>(m_tollContextOverview_default(p_tollContext)),
                tariffTable               := m_optionally_Signed_Unsecured<TariffTables>(
                    { 
                        m_tariffTable_default(
                            { p_tollContextPartitionId },
                            { m_tariff_contextData23 },
                            PX_PAY_UNIT
                        )
                    }
                ),
                chargeReportConfiguration := m_optionally_Signed_Unsecured<ChargeReportConfigurations>({ m_chargeReportConfiguration_contextData23 })
            } // End of template m_contextData23
            
            /**
             * @desc    ContextData23 template for Tariff data element
             * @remark  All other Tariff template shall modify this one
             * @see     [16410-1:2016], Table C.9 —  ContextData23
             */
            template (value) Tariff m_tariff_contextData23 modifies m_tariff_default := {
                roundingRuleForChargeUnitsUsed  := RoundingRule_up_
            }
            
            /**
             * @desc    ChargeReportConfiguration for Context Data #23
             * @see     [17575-3:2016], Context data - Clause 8.3.5.2 EFC attribute ChargeReportConfiguration
             * @see     [16410-1:2016], Table C.9 —  ContextData23
             */
            template (value) ChargeReportConfiguration m_chargeReportConfiguration_contextData23
            modifies m_chargeReportConfiguration_default := {
                chargeReportContent     := m_chargeReportContent_contextData23,
                usageStatementContent   := m_usageStatementContent_contextData23
            } // End of template m_chargeReportConfiguration_contextData23
            
            /**
             * @desc    ChargeReport template for Context Data #23
             * @see     [17575-3:2016], Context data - Clause 8.3.5.2.1 Data type ChargeReport
             * @see     [16410-1:2016], Table C.9 —  ContextData23
             */
            template (value) ChargeReportContent m_chargeReportContent_contextData23
            modifies m_chargeReportContent_default := {
                versionInfo                 := true,
                chargeReportCounter         := true
